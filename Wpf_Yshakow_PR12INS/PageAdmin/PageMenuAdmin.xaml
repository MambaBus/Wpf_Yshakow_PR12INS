<Page x:Class="Wpf_Yshakow_PR12INS.PageAdmin.PageMenuAdmin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Wpf_Yshakow_PR12INS.PageAdmin"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Меню администратора">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                <Button Content="Добавить студента" Style="{StaticResource ButtonB}"/>
                <Button Content="Редактировать студента" Style="{StaticResource ButtonB}"/>
                <Button Content="Удалить студента" Style="{StaticResource ButtonB}"/>
                <Button Content="" Style="{StaticResource ButtonB}"/>
                <Button x:Name="btnBack" Content="Вернуться назад" Style="{StaticResource ButtonB}" Click="btnBack_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
